{
  "expo": {
    "name": "RateFood",
    "slug": "RateFood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f00629"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "MEDIA_LIBRARY",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.johanmarulanda.RateFood",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDg76temFojSy9gkG-T9jCBPXjLarDyt5U"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Personal project where I create a TripAdvisor of Restaurants using react native.",
    "githubUrl": "https://github.com/JohanMarulanda/RateFood"
  }
}
